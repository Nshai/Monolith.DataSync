<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MicroService.Workflow"
                   namespace="IntelliFlo.Platform.Services.Workflow.Domain"
                   default-lazy="true"
                   default-access="property"
                   schema="workflow.dbo">

  <class name="TemplateRole" table="TAssignedRole" >
    <id name="Id" column="AssignedRoleId">
      <generator class="identity"/>
    </id>

    <many-to-one name="TemplateVersion"
                 class="TemplateVersion"
                 column="TemplateVersionId"
                 not-null="true"
                 cascade="none"
                 fetch="select"/>

    <property name="RoleId" />
    <property name="TenantId" />
    <property name="ConcurrencyId" />

  </class>

</hibernate-mapping>

