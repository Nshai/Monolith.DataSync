<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="Reassure:ServiceBaseAddress" value="http://#{service.dnsname}" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="Reassure:WiremockBaseAddress" value="http://#{wiremock.dnsname}:#{wiremock.port}" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
</configuration>